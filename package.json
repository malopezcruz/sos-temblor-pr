{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build && node -p 'process.env.production'",
    "develop": "env-cmd -f .env.development gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "mobile": "gatsby develop -H 0.0.0.0",
    "lambda-build": "netlify-lambda build functions && node -p 'process.env.production'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "gatsby": "^2.13.73",
    "gatsby-cli": "^2.8.22",
    "gatsby-image": "^2.2.17",
    "gatsby-plugin-build-date": "^1.0.0",
    "gatsby-plugin-google-analytics": "^2.1.14",
    "gatsby-plugin-manifest": "^2.2.18",
    "gatsby-plugin-react-helmet": "^3.1.6",
    "gatsby-plugin-react-helmet-canonical-urls": "^1.2.0",
    "gatsby-plugin-sharp": "^2.2.20",
    "gatsby-plugin-sitemap": "^2.2.12",
    "gatsby-source-filesystem": "^2.1.21",
    "gatsby-source-google-spreadsheet": "^2.0.0",
    "gatsby-transformer-sharp": "^2.2.13",
    "google-map-react": "^1.1.5",
    "netlify-lambda": "^1.6.3",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "babel-eslint": "^10.0.3",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.0.1",
    "eslint": "^6.7.2",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-postcss": "^2.1.6",
    "prettier": "^1.18.2",
    "tailwindcss": "^1.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
